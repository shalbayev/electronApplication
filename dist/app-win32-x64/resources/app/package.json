{
  "name": "app",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "dist": "build",
    "build":"electron-packager . app  --platform win32 --arch x64 --overwrite --out dist/ "
  },
  "build": {
    "appId": "yourappid",
    "category": "your.app.category.type",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.4.1",
    "jquery": "^3.4.1",
    "popper.js": "^1.16.1",
    "util.js": "0.0.0",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "electron": "^8.0.1",
    "electron-builder": "^22.3.2",
    "electron-packager": "^14.2.1"
  },
  "description": ""
}
